  -\'c6\'f7\'c0\'ce\'c5\'cd\'b4\'c2 \'b4\'d9\'b8\'a5 \'ba\'af\'bc\'f6\'c0\'c7 \'c1\'d6\'bc\'d2\'b8\'a6 \'c0\'fa\'c0\'e5\'c7\'cf\'b4\'c2 \'ba\'af\'bc\'f6\'c0\'cc\'b4\'d9.\par
  -\'c6\'f7\'c0\'ce\'c5\'cd\'b8\'a6 \'c1\'f6\'bf\'f8\'c7\'cf\'b1\'e2 \'c0\'a7\'c7\'d1 \'bf\'ac\'bb\'ea\'c0\'da\'b4\'c2 &amp;(\'c1\'d6\'bc\'d2),*(\'b3\'bb\'bf\'eb), \'b5\'a5\'c0\'cc\'c5\'cd\'c1\'d6\'bc\'d2\'b8\'a6 \'bf\'c5\'b0\'dc\'c1\'d6\'b4\'c2 +,-\'bf\'ac\'bb\'ea\'c0\'da \'b1\'d7\'b8\'ae\'b0\'ed \'c6\'f7\'c0\'ce\'c5\'cd\'c0\'c7\par
 \'b8\'e2\'b9\'f6  \'ba\'af\'bc\'f6\'b8\'a6 \'b0\'a1\'b8\'a3\'c5\'b3\'bc\'f6 \'c0\'d6\'b4\'c2 -&gt;\'bf\'ac\'bb\'ea\'c0\'da\'b0\'a1 \'c0\'d6\'b4\'d9.\par
\par
